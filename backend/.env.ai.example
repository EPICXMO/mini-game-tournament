# AI Vision Bot Configuration

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# AI Bot Settings
AI_BOT_ENABLED=true
AI_BOT_MAX_CONCURRENT=8
AI_BOT_DEFAULT_PERSONALITY=strategic
AI_BOT_DEFAULT_SKILL_LEVEL=adaptive

# Vision Analysis Settings
AI_VISION_MODEL=gpt-4-vision-preview
AI_VISION_MAX_TOKENS=1500
AI_VISION_TEMPERATURE=0.3
AI_SCREENSHOT_INTERVAL=500
AI_DECISION_DELAY=100

# Decision Making Settings
AI_DECISION_MODEL=gpt-4
AI_DECISION_MAX_TOKENS=800
AI_DECISION_TEMPERATURE=0.4

# Narration Settings
AI_NARRATION_ENABLED=true
AI_NARRATION_MODEL=gpt-4
AI_NARRATION_MAX_TOKENS=300
AI_NARRATION_TEMPERATURE=0.8

# Learning and Memory
AI_LEARNING_ENABLED=true
AI_MAX_MEMORY_SIZE=100
AI_PATTERN_RECOGNITION=true

# Performance Monitoring
AI_PERFORMANCE_TRACKING=true
AI_ANALYTICS_ENABLED=true
AI_LOG_LEVEL=info

# Game-Specific Settings
AI_JETPACK_INTERVAL=300
AI_SUBWAY_INTERVAL=200
AI_GEOMETRY_INTERVAL=100
AI_DINO_INTERVAL=150
AI_SPACE_INTERVAL=250
AI_BUBBLE_INTERVAL=1000